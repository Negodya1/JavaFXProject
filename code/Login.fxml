<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.Controller">
    <BorderPane prefHeight="768.0" prefWidth="1366.0">
        <left>

        </left>
        <bottom>

        </bottom>
        <center>
            <VBox alignment="CENTER" spacing="10" BorderPane.alignment="CENTER">
                <Label text="Username" />
                <TextField fx:id="login" />
                <Label text="Password" />
                <PasswordField fx:id="password" />
                <Button onAction="#login" text="Login" />
                <Button onAction="#registration" text="Registration" />
                <Label fx:id="error" text="" textFill="red" />
            </VBox>
        </center>
        <right>

        </right>
    </BorderPane>
</Group>
